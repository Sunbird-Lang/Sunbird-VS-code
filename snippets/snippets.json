{
  "Function definition": {
    "prefix": "func",
    "body": ["var ${1:name} = func(${2}) {", "\t${3}", "}"]
  },
  "Variable declaration": {
    "prefix": "var",
    "body": ["var ${1:name} = ${2};"]
  },
  "If-Else Statement": {
    "prefix": "if",
    "body": ["if ${1:condition} {", "  ${2}", "} else {", "  ${3}", "}"],
    "description": "Creates an if-else conditional block"
  }
}
